{
    "version": 2,
    "routes": [
        { "src": "/proxy/funnelback", "dest": "/api/server.js" },
        { "src": "/proxy/funnelback/search", "dest": "/api/search.js" },
        { "src": "/proxy/funnelback/tools", "dest": "/api/tools.js" },
        { "src": "/proxy/funnelback/spelling", "dest": "/api/spelling.js" },
        { "src": "/proxy/funnelback/suggest", "dest": "/api/suggest.js" },
        { "src": "/proxy/suggestPeople", "dest": "/api/suggestPeople.js" },
        { "src": "/proxy/suggestPrograms", "dest": "/api/suggestPrograms.js" },

        { "src": "/api/analytics/summary", "dest": "/dashboard/api.js" },
        { "src": "/api/analytics/trends", "dest": "/dashboard/api.js" },
        { "src": "/api/analytics/handlers", "dest": "/dashboard/api.js" },
        { "src": "/api/analytics/zero-results", "dest": "/dashboard/api.js" },
        { "src": "/api/analytics/top-queries", "dest": "/dashboard/api.js" },
        { "src": "/api/analytics/locations", "dest": "/dashboard/api.js" },
        { "src": "/api/analytics/export", "dest": "/dashboard/api.js" },
        
        { "src": "/api/queryCount", "dest": "/api/queryCount.js" },
        { "src": "/api/mongoTest", "dest": "/api/mongoTest.js" },
        { "src": "/api/testAnalytics", "dest": "/api/testAnalytics.js" },
        
        { "src": "/dashboard/static/(.*)", "dest": "/dashboard/frontend/build/static/$1" },
        { "src": "/dashboard/manifest.json", "dest": "/dashboard/frontend/build/manifest.json" },
        { "src": "/dashboard/favicon.ico", "dest": "/dashboard/frontend/build/favicon.ico" },
        { "src": "/dashboard/logo192.png", "dest": "/dashboard/frontend/build/logo192.png" },
        { "src": "/dashboard/logo512.png", "dest": "/dashboard/frontend/build/logo512.png" },
        { "src": "/dashboard/asset-manifest.json", "dest": "/dashboard/frontend/build/asset-manifest.json" },
        { "src": "/dashboard/robots.txt", "dest": "/dashboard/frontend/build/robots.txt" },
        
        { "src": "/dashboard(.*)", "dest": "/dashboard/frontend/build/index.html" }
    ]
}